<FCrossDoubleInput>:
    name:"FCrossDoubleInput"
    across: a
    apos: ap
    alen: al
    down: d
    dpos: dp 
    dlen: dl
    Label:
        text:"Cross Word Helper"
        font_size:35
        # font_name:'Comic'
        pos_hint:{"x":0.45,"y":0.85}
        size_hint:0.1,0.1
        underline:True

    Label:
        text:"Across"
        font_size:25
        # font_name:'Comic'
        pos_hint:{"x":0.45,"y":0.7}
        size_hint:0.1,0.1
        halign:"center"
        
    EnterWord:
        id:a
        multiline:False
        pos_hint:{"x":0.05,"y":0.6}
        hint_text:"Enter Description"
   
    EnterWord:
        id:ap
        multiline:False
        pos_hint:{"x":0.35,"y":0.6}
        hint_text:"Shared Position"

    EnterWord:
        id:al
        multiline:False
        pos_hint:{"x":0.65,"y":0.6}
        hint_text:"Length "

    Label:
        text:"Down"
        font_size:25
        # font_name:'Comic'
        pos_hint:{"x":0.45,"y":0.45}
        size_hint:0.1,0.1
        halign:"center"
        
    EnterWord:
        id:d
        multiline:False
        pos_hint:{"x":0.05,"y":0.35}
        hint_text:"Enter Description"
   
    EnterWord:
        id:dp
        multiline:False
        pos_hint:{"x":0.35,"y":0.35}
        hint_text:"Shared Position"

    EnterWord:
        id:dl
        multiline:False
        pos_hint:{"x":0.65,"y":0.35}
        hint_text:"Length "

    GreenButton:
        text:"Back"
        on_release:app.root.current="FindWindow"
        pos_hint:{"x":0.2,"y":0.15}

    GreenButton:
        text:"Submit"
        on_press:
            root.manager.get_screen('FCrossDoubleRes').res.text =root.cross(a.text,ap.text,al.text,d.text,dp.text,dl.text)
        on_release:app.root.current="FCrossDoubleRes"
        pos_hint:{"x":0.6,"y":0.15}

