<MainWindow>:
    name:"main"
    GridLayout:
        cols:3
    
    Label:
        text:"WELCOME TO WORDZONE !"
        font_size:45
        # font_name:'Comic'
        pos_hint:{"x":0.45,"y":0.8}
        size_hint:0.1,0.1
        underline:True

    SmoothButton:
        text:"Find\nZone"
        bold: True
        on_release:app.root.current="FindWindow"
        pos_hint:{"x":0.1,"y":0.35}
        size_hint:0.2,0.2
        font_size:20
        color:1,1,1,1
        text_size: self.size     
        halign: 'center'
        valign: 'center'
        background_normal: ''
        background_color: 1, 1, 1, 0
        border: 0, 32, 0, 32
        size: 200, 64
        center: root.width / 2, root.height / 2
        
    SmoothButton:
        text:"Understand\nZone"
        
        on_release:app.root.current="UnderstandWindow"
        pos_hint:{"x":0.4,"y":0.35}
        size_hint:0.2,0.2
        font_size:20
        color:1,1,1,1
        text_size: self.size     
        halign: 'center'
        valign: 'center'
        background_normal: ''
        background_color: 1, 1, 1, 0
        # background_color: utils.get_color_from_hex('#58AE6F')
        border:0.1,0.1,0.2,0.4
    SmoothButton:
        text:"Knowledge\nZone"
        
        on_release:app.root.current="KnowWindow"
        pos_hint:{"x":0.7,"y":0.35}
        size_hint:0.2,0.2
        font_size:20
        color:1,1,1,1
        text_size: self.size     
        halign: 'center'
        valign: 'center'
        background_normal: ''
        background_color: 1, 1, 1, 0
        border:0.1,0.1,0.2,0.4






